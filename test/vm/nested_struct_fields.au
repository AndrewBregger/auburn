struct Bar {
	pub foo Foo
	pub f5 f32
}

struct Foo {
	pub f1 f32
	pub f2 i32
	pub f3 f32
	pub f4 i32
}

fn main() {
	let bar = Bar {
		foo: Foo {
			f1: 1.0,
			f2: 2,
			f3: 3.1,
		f4: 4
		},
		f5: 3.1415
	}

	echo bar.foo.f1
	echo bar.f5
	echo bar.foo.f2
	echo bar.foo.f3
	echo bar.foo.f4
}
